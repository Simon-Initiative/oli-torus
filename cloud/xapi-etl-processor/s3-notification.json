{
  "LambdaConfigurations": [
    {
      "Id": "XAPIEventProcessing",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:ACCOUNT_ID:function:process_xapi_events-prod",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".jsonl"
            },
            {
              "Name": "prefix",
              "Value": "section/"
            }
          ]
        }
      }
    }
  ]
}