  "scripts": {
    "analyze-bundle": "BUNDLE_ANALYZER_ENABLED=true node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --mode production",
    "deploy": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --mode production",
    "deploy-node": "webpack --mode production --config webpack.config.node.js",
    "watch": "webpack --mode development --watch",
    "build": "npm run clean && npm run lint && npm run test && npm run deploy",
    "clean": "del --force coverage/** ../priv/static",
    "lint": "eslint src test --rule 'react-hooks/exhaustive-deps: off' --rule '@typescript-eslint/no-non-null-assertion: off'",
    "lint:fix": "npm run lint -- --rule 'react-hooks/exhaustive-deps: off' --rule '@typescript-eslint/no-non-null-assertion: off' --fix",
    "prettier": "prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch -o --coverage=false",
    "type-coverage": "type-coverage",
    "check-types": "tsc --noemit --skipLibCheck",
    "sdk-docs": "npx typedoc --tsconfig tsconfig.sdk.json --plugin typedoc-plugin-markdown  --out typedocs src/components/activities/index.ts",
    "//": "Note: Under normal usage, tailwind is managed by phoenix, this is only for storybook",
    "build-storybook-tailwind": "tailwindcss -c tailwind.config.js -i css/app.css -o .storybook/temp/app.css",
    "storybook:watch": "start-storybook -p 6006",
    "tailwind:watch": "tailwindcss -c tailwind.config.js -i css/app.css -o .storybook/temp/app.css --watch",
    "storybook": "concurrently \"yarn tailwind:watch\" \"yarn storybook:watch\"",
    "build-storybook": "yarn build-storybook-tailwind && build-storybook -o ../doc/storybook-static "
  },
